import "base"
import "auth"
import "match"

model MatchScoreEntry extends AuditableBase {
    scorerId String
    scorer User @relation(fields: [scorerId], references: [id])

    matchId String
    match Match @relation(fields: [matchId], references: [id])

    scoreData Json
}

model ManualPointsAdjustment extends AuditableBase {
    issuerId String
    issuer User @relation(fields: [issuerId], references: [id])

    leaguePoints Int

    reason String
}