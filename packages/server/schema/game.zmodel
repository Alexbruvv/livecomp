import "base"
import "competition"
import "match"

model Game extends AuditableBase {
    name String

    startingZones StartingZone[]
    competitions Competition[]
}

model StartingZone extends AuditableBase {
    name String
    color String

    gameId String
    game Game @relation(fields: [gameId], references: [id])

    assignments MatchAssignment[]
}