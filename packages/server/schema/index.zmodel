import "auth"
import "competition"
import "game"
import "match"
import "scoring"
import "team"
import "venue"

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

generator prismabox {
    provider = "prismabox"

    typeboxImportVariableName = "t"
    typeboxImportDependencyName = "elysia"
}