import "base"
import "team"
import "venue"
import "game"
import "match"

model Competition extends AuditableBase {
    name String
    shortName String
    startsAt DateTime
    endsAt DateTime

    gameId String
    game Game @relation(fields: [gameId], references: [id])

    venueId String
    venue Venue @relation(fields: [venueId], references: [id])

    teams Team[]

    matchPeriods MatchPeriod[]
}

